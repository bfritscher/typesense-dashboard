{
  "name": "typesense-dashboard",
  "version": "2.0.4",
  "description": "A Typesense Dashboard to manage and browse collections.",
  "productName": "Typesense-Dashboard",
  "author": "Boris Fritscher <boris@fritscher.ch>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:desktop": "quasar dev -m electron --devtools",
    "build": "quasar build",
    "build:desktop": "quasar build -m electron --target all",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "1.16.17",
    "axios": "1.8.4",
    "file-saver": "2.0.5",
    "instantsearch.css": "8.5.1",
    "monaco-editor": "0.52.2",
    "nanoid": "5.1.5",
    "pinia": "3.0.2",
    "pretty-bytes": "6.1.1",
    "quasar": "2.18.1",
    "typesense": "2.0.3",
    "typesense-instantsearch-adapter": "2.9.0",
    "vue": "3.5.13",
    "vue-instantsearch": "4.20.6",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@electron/packager": "18.3.6",
    "@eslint/js": "9.24.0",
    "@quasar/app-vite": "2.2.0",
    "@types/file-saver": "2.0.7",
    "@types/node": "22.14.1",
    "@vue/devtools": "7.7.2",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "autoprefixer": "10.4.21",
    "electron": "35.1.5",
    "eslint": "9.24.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite-plugin-checker": "0.9.1",
    "vue-tsc": "2.2.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.40.0",
    "sass-embedded": "1.86.3"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
