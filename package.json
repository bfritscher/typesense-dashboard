{
  "name": "typesense-dashboard",
  "version": "2.2.1",
  "description": "A Typesense Dashboard to manage and browse collections.",
  "productName": "Typesense-Dashboard",
  "author": "Boris Fritscher <boris@fritscher.ch>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:desktop": "quasar dev -m electron --devtools",
    "build": "quasar build",
    "build:desktop": "quasar build -m electron --target all",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "1.17.0",
    "axios": "1.12.2",
    "file-saver": "2.0.5",
    "instantsearch.css": "8.5.1",
    "monaco-editor": "0.53.0",
    "nanoid": "5.1.6",
    "pinia": "3.0.3",
    "pretty-bytes": "7.1.0",
    "quasar": "2.18.5",
    "typesense": "2.1.0",
    "typesense-instantsearch-adapter": "2.9.0",
    "vue": "3.5.22",
    "vue-instantsearch": "4.21.3",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@electron/packager": "18.4.4",
    "@eslint/js": "9.36.0",
    "@quasar/app-vite": "2.4.0",
    "@types/file-saver": "2.0.7",
    "@types/node": "24.5.2",
    "@vue/devtools": "8.0.2",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "autoprefixer": "10.4.21",
    "electron": "38.1.2",
    "eslint": "9.36.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vite-plugin-checker": "0.10.3",
    "vue-tsc": "3.0.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.2",
    "sass-embedded": "1.93.2"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
