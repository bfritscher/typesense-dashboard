{
  "name": "typesense-dashboard",
  "version": "2.4.2",
  "description": "A Typesense Dashboard to manage and browse collections.",
  "productName": "Typesense-Dashboard",
  "author": "Boris Fritscher <boris@fritscher.ch>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:desktop": "quasar dev -m electron --devtools",
    "build": "quasar build",
    "build:desktop": "quasar build -m electron --target all",
    "postinstall": "quasar prepare",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "@quasar/extras": "1.17.0",
    "axios": "1.13.2",
    "file-saver": "2.0.5",
    "instantsearch.css": "8.9.0",
    "monaco-editor": "0.55.1",
    "nanoid": "5.1.6",
    "pinia": "3.0.4",
    "pretty-bytes": "7.1.0",
    "quasar": "2.18.6",
    "typesense": "2.1.0",
    "typesense-instantsearch-adapter": "2.9.0",
    "vue": "3.5.26",
    "vue-instantsearch": "4.22.6",
    "vue-router": "4.6.4"
  },
  "devDependencies": {
    "@electron/packager": "18.4.4",
    "@eslint/js": "9.39.2",
    "@quasar/app-vite": "2.4.0",
    "@types/file-saver": "2.0.7",
    "@types/node": "25.0.3",
    "@vue/devtools": "8.0.5",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "autoprefixer": "10.4.23",
    "electron": "39.2.7",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "10.6.2",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "vite-plugin-checker": "0.12.0",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.1.8"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
